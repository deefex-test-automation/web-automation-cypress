{
  "name": "web-browser-automation",
  "version": "1.0.0",
  "description": "An example of web browser automation with cypress and a page object model approach",
  "main": "index.js",
  "scripts": {
    "cy": "./node_modules/.bin/cypress open",
    "cy:e2e:chrome": "./node_modules/.bin/cypress run --browser chrome --spec \"cypress/e2e/*.cy.ts\"",
    "cy:e2e:firefox": "./node_modules/.bin/cypress run --browser firefox --spec \"cypress/e2e/*.cy.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deefex-test-automation/web-browser-automation.git"
  },
  "keywords": [
    "test",
    "automation",
    "cypress",
    "browser"
  ],
  "author": "Del Dewar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/deefex-test-automation/web-browser-automation/issues"
  },
  "homepage": "https://github.com/deefex-test-automation/web-browser-automation#readme",
  "dependencies": {
    "cypress": "^10.8.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0",
    "typescript": "^4.8.3"
  },
  "devDependencies": {
    "@types/mochawesome": "^6.2.1",
    "cypress-mochawesome-reporter": "^3.2.2"
  }
}
